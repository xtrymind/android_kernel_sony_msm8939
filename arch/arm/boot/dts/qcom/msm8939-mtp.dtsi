	gen-vkeys {
		compatible = "qcom,gen-vkeys";
		label = "synaptics_dsx";
		qcom,disp-maxx = <1079>;
		qcom,disp-maxy = <1919>;
		qcom,panel-maxx = <1079>;
		qcom,panel-maxy = <2084>;
		qcom,key-codes = <158 139 172 217>;
	};

	i2c@78ba000 { /* BLSP1 QUP6 */
		nxp@28 {
			compatible = "nxp,pn544";
			reg = <0x28>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <21 0x2>;
			nxp,irq-gpio=<&msm_gpio 21 0>;
			nxp,vengpio= <&msm_gpio 20 0>;
			nxp,firmgpio_EVM=<&msm_gpio 112 0>;
			nxp,firmgpio=<&msm_gpio 120 0>;
		};
	};
	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		pinctrl-names = "tlmm_gpio_key_active","tlmm_gpio_key_suspend";
		pinctrl-0 = <&gpio_key_active>;
		pinctrl-1 = <&gpio_key_suspend>;
		/* PERI-JC-KEYPAD_BringUp-00+[ */
		camera_focus {
			label = "camera_focus";
			gpios = <&msm_gpio 108 0x1>;
			linux,input-type = <1>;
			linux,code = <0x210>;
			gpio-key,wakeup;
			debounce-interval = <100>;
		};

		camera_snapshot {
			label = "camera_snapshot";
			gpios = <&msm_gpio 109 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <100>;
		};

		vol_down {
			label = "volume_down";
			gpios = <&msm_gpio 51 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			gpio-key,wakeup;
			debounce-interval = <100>;
		};

		vol_up {
			label = "volume_up";
			gpios = <&msm_gpio 11 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			gpio-key,wakeup;
			debounce-interval = <100>;
		};
		/* PERI-JC-KEYPAD_BringUp-00+] */
		};
	 /* PERI-JC-CombineKey-00+[ */
	 keyreset {
	 	compatible = "keycombo";
	 	down,num = <2>;
	 	down,code = <115 116>;
	 	down,codeV = <114 116>;
	};
	 /* PERI-JC-CombineKey-00+] */	

	i2c@78b6000 { /* BLSP1 QUP2 */

		bosch@10 {
			compatible = "bosch,bma2x2";
			reg = <0x10>;

			pinctrl-names = "bma_default","bma_sleep";
			pinctrl-0 = <&bma2x2_default>;
			pinctrl-1 = <&bma2x2_sleep>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <69 0x00>;

			bosch,int_pin = <&msm_gpio 69 0x00>;
			bosch,layout=<0x8>;
			bmavdd-supply = <&pm8916_l17>;
			bmavio-supply = <&pm8916_l6>;
			bosch,vdd-current-level = <1 210>;
		};

		bosch@12 {
			compatible = "bosch,bmm050";
			reg = <0x12>;

			bmmvdd-supply = <&pm8916_l17>;
			bmmvio-supply = <&pm8916_l6>;
			bosch,vdd-current-level = <1 255>;
		};

	    lp5562@30 {
	        compatible = "ti,lp5562";
	        reg = <0x30>;
                enable-gpio = <&msm_gpio 32 0>;
            pinctrl-names = "tlmm_lp5562_active","tlmm_lp5562_suspend";
            pinctrl-0 = <&lp5562_active>;
            pinctrl-1 = <&lp5562_suspend>;
	        clock-mode = /bits/8 <2>;
	    
	        chan0 {
	            chan-name = "R";
	            led-cur = /bits/ 8 <0x20>;
	            max-cur = /bits/ 8 <0x60>;
	        };
	    
	        chan1 {
	            chan-name = "G";
	            led-cur = /bits/ 8 <0x20>;
	            max-cur = /bits/ 8 <0x60>;
	        };
	    
	        chan2 {
	            chan-name = "B";
	            led-cur = /bits/ 8 <0x20>;
	            max-cur = /bits/ 8 <0x60>;
	        };
	    
	        chan3 {
	            chan-name = "W";
	            led-cur = /bits/ 8 <0x20>;
	            max-cur = /bits/ 8 <0x60>;
	        };
	    };	

		cm36286@60 {
			compatible = "capella,cm36286";
			reg = <0x60>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <113 0x2>;
			pinctrl-names = "pls_active","pls_suspend";
			pinctrl-0 = <&cm_int_active>;
			pinctrl-1 = <&cm_int_suspend>;
			intr = <&msm_gpio 113 0>;
			cm36286,slave_addr = <0x60>;
			cm36286,levels = <0x0A 0xA0 0xE1 0x140 0x280 0x500 0xA28 0x16A8 0x1F40 0x2800>;
			cm36286,ps_close_thd_set = <0xffff>;
			cm36286,ps_away_thd_set = <0x1>;
			cm36286,ls_thd_high_offset = <0xa>;
			cm36286,ls_thd_low_offset = <0xa>;
			vdd-supply = <&pm8916_l17>;
			vcc_i2c-supply = <&pm8916_l6>;
			cm36286,vdd_power = < 2850000 2850000 15000 >;
			cm36286,i2c_power = < 1800000 1800000 15000 >;

			/* CM36686_ALS_IT_160ms | CM36686_ALS_PERS_4 */
			/* 0x40 | 0x8  */
			cm36286,ls_cmd = <0x48>;

			/* CM36686_PS_IT_1T | CM36686_PS_DR_1_40 | CM36686_PS_PERS_2 */
			/* 0 | 0 | 0x10 */
			cm36286,ps_conf1_val = <0x10>;

			/* CM36686_PS_MS_NORMAL | CM36686_LED_I_75 */
			/* 0 | 0x100 */
			cm36286,ps_conf3_val = <0x100>;
		};
	};
};

&blsp1_uart2 {
	status = "ok";
	pinctrl-names = "default";
	pinctrl-0 = <&uart_console_sleep>;
};

&pm8916_gpios {
	gpio@c100 { /* GPIO 2 */
		/* NFC_CLK_REQ */
		qcom,mode = <0>; /* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>; /* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>; /* QPNP_PIN_VIN2 */
		qcom,src-sel = <2>; /* QPNP_PIN_SEL_FUNC_1 */
		qcom,master-en = <1>;
	};
};
&spmi_bus {
	qcom,pm8916@1 {
		qcom,vibrator@c000 {
			status = "okay";
			qcom,vib-timeout-ms = <15000>;
			qcom,vib-vtg-level-mV = <3100>;
		};
	};

	qcom,pm8916@0 {
		qcom,leds@a100 {
			status = "okay";
			qcom,led_mpp_2 {
				label = "mpp";
				linux,name = "button-backlight";
				linux,default-trigger = "none";
				qcom,default-state = "off";
				qcom,max-current = <40>;
				qcom,current-setting = <5>;
				qcom,id = <6>;
				qcom,mode = "manual";
				qcom,source-sel = <1>;
				qcom,mode-ctrl = <0x60>;
			};
		};
	};
};

&sdhc_1 {
	vdd-supply = <&pm8916_l8>;
	qcom,vdd-voltage-level = <2900000 2900000>;
	qcom,vdd-current-level = <200 400000>;

	vdd-io-supply = <&pm8916_l5>;
	qcom,vdd-io-always-on;
	qcom,vdd-io-lpm-sup;
	qcom,vdd-io-voltage-level = <1800000 1800000>;
	qcom,vdd-io-current-level = <200 60000>;

	pinctrl-names = "active", "sleep";
	pinctrl-0 = <&sdc1_clk_on &sdc1_cmd_on &sdc1_data_on>;
	pinctrl-1 = <&sdc1_clk_off &sdc1_cmd_off &sdc1_data_off>;

	qcom,nonremovable;

	status = "ok";
};

&sdhc_2 {
	vdd-supply = <&pm8916_l11>;
	qcom,vdd-voltage-level = <2800000 2950000>;
	qcom,vdd-current-level = <15000 400000>;

	vdd-io-supply = <&pm8916_l12>;
	qcom,vdd-io-voltage-level = <1800000 2950000>;
	qcom,vdd-io-current-level = <200 50000>;

	pinctrl-names = "active", "sleep";
	pinctrl-0 = <&sdc2_clk_on &sdc2_cmd_on &sdc2_data_on &sdc2_cd_on>;
	pinctrl-1 = <&sdc2_clk_off &sdc2_cmd_off &sdc2_data_off &sdc2_cd_off>;

	#address-cells = <0>;
	interrupt-parent = <&sdhc_2>;
	interrupts = <0 1 2>;
	#interrupt-cells = <1>;
	interrupt-map-mask = <0xffffffff>;
	interrupt-map = <0 &intc 0 125 0
			1 &intc 0 221 0
			2 &msm_gpio 38 0>;
	interrupt-names = "hc_irq", "pwr_irq", "status_irq";
	cd-gpios = <&msm_gpio 38 0x1>;

	status = "ok";
};

&pm8916_chg {
	status = "ok";
	qcom,charging-disabled;
};

